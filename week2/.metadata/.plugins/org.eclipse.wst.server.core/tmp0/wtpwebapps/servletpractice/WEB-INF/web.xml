    <servlet-name>SqServ</servlet-name>
    <servlet-class>com.revature.servletpractice.SqServ</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SqServ</servlet-name>
    <url-pattern>/sq</url-pattern>
  </servlet-mapping>
